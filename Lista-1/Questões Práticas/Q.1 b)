nA = Node("A")
nB = Node("B")
nC = Node("C")
nD = Node("D")
nE = Node("E")
nF = Node("F")

graph = Graph()

graph.add_node(nA)
graph.add_node(nB)
graph.add_node(nC)
graph.add_node(nD)
graph.add_node(nE)
graph.add_node(nF)

e1 = Edge("A-B", 4, nA, nB)
e2 = Edge("A-C", 4, nA, nC)
e3 = Edge("B-C", 2, nB, nC)
e4 = Edge("B-D", 1, nB, nD)
e5 = Edge("C-E", 1, nC, nE)
e6 = Edge("D-E", 1, nD, nE)
e7 = Edge("D-F", 2, nD, nF)
e8 = Edge("E-F", 4, nE, nF)

graph.add_edge(e1)
graph.add_edge(e2)
graph.add_edge(e3)
graph.add_edge(e4)
graph.add_edge(e5)
graph.add_edge(e6)
graph.add_edge(e7)
graph.add_edge(e8)

graph.print_graph()
